{
  "name": "@fleettools/fleet-cli",
  "version": "0.1.0",
  "description": "FleetTools Global CLI - Fleet management and project bootstrapping",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "fleet": "dist/index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc && bun run build:types",
    "build:types": "tsc --emitDeclarationOnly",
    "build:bun": "bun build src/index.ts --outdir dist --target bun --format esm",
    "dev": "bun --watch src/index.ts",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "prepublishOnly": "bun run build"
  },
  "dependencies": {
    "commander": "^11.1.0",
    "yaml": "^2.3.4",
    "chalk": "^5.3.0",
    "inquirer": "^9.2.12",
    "@fleettools/fleet-shared": "^0.1.0",
    "@fleettools/core": "^0.1.0"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/node": "^25.0.9",
    "typescript": "^5.9.3"
  },
  "files": [
    "dist",
    "README.md",
    "templates/**/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "bun": ">=1.0.0",
    "node": ">=18.0.0"
  },
  "keywords": [
    "fleettools",
    "cli",
    "ai",
    "agents",
    "coordination"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/v1truv1us/fleettools.git",
    "directory": "packages/fleet-cli"
  },
  "bugs": {
    "url": "https://github.com/v1truvius/fleettools/issues"
  },
  "homepage": "https://github.com/v1truvius/fleettools#readme"
}
